{
  "name": "WorkAround",
  "version": "0.1.0",
  "description": "Work around paywalls",
  "permissions": ["tabs"],
  "host_permissions": ["https://*.nytimes.com/*", "https://*.nymag.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*.nytimes.com/*", "https://*.nymag.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "icons": {
    "16": "wario_W_16.png",
    "48": "wario_W_48.png",
    "128": "wario_W_128.png"
  },
  "manifest_version": 3
}
